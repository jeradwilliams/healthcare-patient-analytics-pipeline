{
  "expectation_suite_name": "patient_admissions_suite",
  "expectations": [
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 50000,
        "max_value": 100000,
        "table": "raw.admissions"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "patient_id",
        "table": "raw.patients"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "birth_date",
        "min_value": "1900-01-01",
        "max_value": "2025-12-31",
        "parse_strings_as_dates": true,
        "table": "raw.patients"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "diagnosis_code",
        "regex": "^[A-Z]\\d{2}(\\.\\d{1,4})?$",  // Basic ICD-like code pattern
        "table": "raw.admissions"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "gender",
        "value_set": ["M", "F"],
        "table": "raw.patients"
      }
    }
  ]
}
